{
  "name": "camo",
  "version": "1.0.0",
  "description": "Camoufox Browser CLI - Cross-platform browser automation",
  "type": "module",
  "bin": {
    "camo": "./bin/camo.mjs"
  },
  "scripts": {
    "build": "node scripts/build.mjs",
    "test": "node --test tests/*.test.mjs",
    "install:global": "npm run build && npm install -g .",
    "uninstall:global": "npm uninstall -g camo",
    "build:global": "npm run build && npm test && npm install -g ."
  },
  "keywords": ["browser", "automation", "camoufox", "cli", "playwright"],
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {},
  "peerDependencies": {
    "camoufox": "^0.1.0"
  }
}
