{
  "name": "@webauto/camo",
  "version": "0.1.0001",
  "description": "Camoufox Browser CLI - Cross-platform browser automation",
  "type": "module",
  "bin": {
    "camo": "./bin/camo.mjs"
  },
  "files": [
    "bin/",
    "src/",
    "scripts/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "node scripts/build.mjs",
    "test": "node --test tests/*.test.mjs",
    "version:bump": "node scripts/bump-version.mjs",
    "install:global": "npm run build && npm install -g .",
    "uninstall:global": "npm uninstall -g @webauto/camo",
    "build:global": "npm run build && npm test && npm install -g ."
  },
  "keywords": [
    "browser",
    "automation",
    "camoufox",
    "cli",
    "playwright"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {},
  "peerDependencies": {
    "camoufox": "^0.1.0"
  }
}
